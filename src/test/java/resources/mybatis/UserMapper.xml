<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ant.UserMapper">
    <select id="selectUser" parameterType="java.lang.String" resultType="com.ant.User">
          select * from user where id = #{id}
    </select>

    <select id="selectUsers" parameterType="java.lang.String" resultType="com.ant.User">
        select * from user
    </select>
</mapper>